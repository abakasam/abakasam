function phone(number) { 
	var numbers = '0123456789'
	number = number + '' 
	var dial = ''
	
	for(var index = 0; index < number.length; index++) { 
		var exist = false
		
		for(var indexB = 0; indexB < numbers.length; indexB++) { 
			if(number[index] == numbers[indexB]) exist = true
		} 
		
		if(exist == false) { 
		} else { 
			dial = dial + number[index]
		}
	} 
	
	return dial
}

